{
    "name": "minaalfy/laravel-docker",
    "description": "Adds docker support for Laravel projects including the following services: MariaDB, Redis, Supervisord, and PHP/Laravel.",
    "keywords": [
        "laravel",
        "docker"
    ],
    "authors": [
        {
            "name": "Mina Alfy",
            "email": "minaalfy8@gmail.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Minaalfy\\LaravelDocker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minaalfy\\LaravelDocker\\LaravelDockerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}